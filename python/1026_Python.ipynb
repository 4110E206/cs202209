        "def sum(i1, i2):\n",
        "    result = 0\n",
        "    for i in range(i1,i2):\n",
        "      result += i\n",
        "    return result\n",
        "\n",
        "def main():\n",
        "    print(\"Sum from 3 to 22 is\", sum(3, 22)) \n",
        "    print(\"Sum from 18 to 40 is\", sum(18, 40))\n",
        "    print(\"Sum from 23 to 31 is\", sum(23, 31))\n",
        "\n",
        "main() # Call the main function"
